
	.data
mm_:	.word 0

	.text 			# The .text assembler directive indicates
	.globl fibonacci_		# The following is the code (as oppose to data)
fibonacci_:				# .globl makes main know to the 
				# outside of the program.
# Prologue begins 
	sw $ra, 0($sp)		# Save the return address
	sw $fp, -4($sp)		# Save the frame pointer
	sub $fp, $sp, 8		# Update the frame pointer

	sub $sp, $sp, 20		# Make space for the locals
# Prologue ends

	lw $v0, 12($fp)
	li $t0, 0
	seq $t1, $v0, $t0
	lw $v0, 12($fp)
	li $t0, 1
	seq $t2, $v0, $t0
	or $v0, $t1, $t2
	beq $v0, $zero, label1 
	li $t0, 1
	sw $t0, -8($fp)
	lw $t0, -8($fp)
	move $v1, $t0
	j epilogue_fibonacci_

label1:    	
	lw $v0, 12($fp)
	li $t0, 1
	sub $t1, $v0, $t0
	sw $t1, 0($fp)
	lw $v0, 0($fp)
	sw $v0, 0($sp)
	sub $sp, $sp, 4
	jal fibonacci_
	add $sp, $sp, 4
	move $v0, $v1
	sw $v0, 0($fp)
	lw $v0, 12($fp)
	li $t0, 2
	sub $t1, $v0, $t0
	sw $t1, -4($fp)
	lw $v0, -4($fp)
	sw $v0, 0($sp)
	sub $sp, $sp, 4
	jal fibonacci_
	add $sp, $sp, 4
	move $v0, $v1
	sw $v0, -4($fp)
	lw $v0, 0($fp)
	lw $t0, -4($fp)
	add $t1, $v0, $t0
	sw $t1, -8($fp)
	lw $v0, -8($fp)
	move $v1, $v0
	j epilogue_fibonacci_

# Epilogue Begins
epilogue_fibonacci_:
	add $sp, $sp, 20
	lw $fp, -4($sp)  
	lw $ra, 0($sp)   
	jr        $31		# Jump back to the called procedure
# Epilogue Ends


	.text 			# The .text assembler directive indicates
	.globl main		# The following is the code (as oppose to data)
main:				# .globl makes main know to the 
				# outside of the program.
# Prologue begins 
	sw $ra, 0($sp)		# Save the return address
	sw $fp, -4($sp)		# Save the frame pointer
	sub $fp, $sp, 8		# Update the frame pointer

	sub $sp, $sp, 12		# Make space for the locals
# Prologue ends

	li $v0, 4
	sw $v0, 0($fp)
	lw $v0, 0($fp)
	sw $v0, 0($sp)
	sub $sp, $sp, 4
	jal fibonacci_
	add $sp, $sp, 4
	move $v0, $v1
	sw $v0, 0($fp)
	li $v0, 1
	lw $a0, 0($fp)
	syscall

# Epilogue Begins
epilogue_main:
	add $sp, $sp, 12
	lw $fp, -4($sp)  
	lw $ra, 0($sp)   
	jr        $31		# Jump back to the called procedure
# Epilogue Ends

