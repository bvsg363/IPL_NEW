
	.data
area:	.word 0
perimeter:	.word 0

	.text 			# The .text assembler directive indicates
	.globl main		# The following is the code (as oppose to data)
main:				# .globl makes main know to the 
				# outside of the program.
# Prologue begins 
	sw $fp, 0($sp)		# Save the frame pointer
	sub $fp, $sp, 4		# Update the frame pointer

	sub $sp, $sp, 24		# Make space for the locals
# Prologue ends

	li.d $f2, 3.00
	s.d $f2, -12($fp)
	li $v0, 2
	sw $v0, 0($fp)
	li.d $f2, 4.00
	s.d $f2, area
	li $v0, 1
	li.d $f2, 2.00
	li.d $f4, 3.40
	slt $t0, $f2, $f4
	sltu $t1, $t0, $v0
	li $v0, 2
	li $t0, 3
	sgt $t2, $v0, $t0
	or $v0, $t1, $t2
	beq $v0, $zero, label6 
	li $t0, 4
	or $t2, $t0, $zero
	j label7

label6:    	
	li $t1, 6
	or $t2, $t1, $zero

label7:    	
	sw $t2, 0($fp)
	lw $v0, 0($fp)
	li $t0, 5
	sle $t1, $v0, $t0
	lw $v0, 0($fp)
	li $t0, 4
	slt $t2, $v0, $t0
	lw $v0, 0($fp)
	li $t0, 5
	sgt $t3, $v0, $t0
	and $v0, $t2, $t3
	or $t0, $t1, $v0
	beq $t0, $zero, label8 
	li $v0, 8
	sw $v0, 0($fp)
	j label9

label8:    	
	li $v0, 9
	sw $v0, 0($fp)
	li $v0, 6
	sw $v0, -4($fp)

label9:    	

label10:    	
	lw $v0, 0($fp)
	li $t0, 5
	sle $t2, $v0, $t0
	lw $v0, 0($fp)
	li $t0, 4
	slt $t3, $v0, $t0
	or $v0, $t2, $t3
	lw $t0, 0($fp)
	li $t2, 5
	sgt $t3, $t0, $t2
	or $t0, $v0, $t3
	beq $t0, $zero, label12 
	li $v0, 8
	sw $v0, 0($fp)
	j label13

label12:    	
	li $v0, 9
	sw $v0, 0($fp)

label13:    	

label11:    	
	lw $v0, 0($fp)
	li $t0, 4
	seq $t1, $v0, $t0
	bne $t1, $zero, label10 

# Epilogue Begins
	add $sp, $sp, 24
	lw $fp, 0($sp)  
	jr        $31		# Jump back to the operating system.
# Epilogue Ends

